void setup()          //not sure if this is the right code because did not understand any other methods to interface servo
{
 pinMode(9,OUTPUT);
}

void loop()
{
 digitalWrite(9,HIGH);
  delayMicroseconds(2200);  //sets servo to 180 degrees
  digitalWrite(9,LOW);
  delay(10);
  
  digitalWrite(9,HIGH);
  delayMicroseconds(1500); //sets servo to mid point
  digitalWrite(9,LOW);
  delay(10);
  
  
}
